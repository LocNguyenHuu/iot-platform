    "id": {
      "type": "string",
      "id": true,
      "required": true
    },
    "creation": {
      "type": "number"
    },
    "name": {
      "type": "string"
    },
    "device_type": {
      "type": "string"
    },
    "parsers": {
      "type": [
        "Parser"
      ]
    }
  },
  "validations": [],
